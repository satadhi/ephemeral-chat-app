{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "consistent-hashing": {
      "type": "library",
      "root": "libs/consistent-hashing",
      "entryFile": "index",
      "sourceRoot": "libs/consistent-hashing/src",
      "compilerOptions": {
        "tsConfigPath": "libs/consistent-hashing/tsconfig.lib.json"
      }
    },
    "common-interfaces": {
      "type": "library",
      "root": "libs/common-interfaces",
      "entryFile": "index",
      "sourceRoot": "libs/common-interfaces/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-interfaces/tsconfig.lib.json"
      }
    }
  }
}